{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.eslint.json"

    },
    "ignorePatterns": [
        "node_modules/",
        "dist/"
    ],
    "rules": {
        "semi": "off",
        "@typescript-eslint/semi": "warn",
        "@typescript-eslint/explicit-function-return-type": "off"
    }
}
